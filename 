[32m+[m[32m    public static function getSystemCaRootBundlePath()[m
[32m+[m[32m    {[m
[32m+[m[32m        static $caPath = null;[m
[32m+[m
[32m+[m[32m        if ($caPath !== null) {[m
[32m+[m[32m            return $caPath;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // If SSL_CERT_FILE env variable points to a valid certificate/bundle, use that.[m
[32m+[m[32m        // This mimics how OpenSSL uses the SSL_CERT_FILE env variable.[m
[32m+[m[32m        $envCertFile = getenv('SSL_CERT_FILE');[m
[32m+[m[32m        if ($envCertFile && is_readable($envCertFile) && validateCaFile(file_get_contents($envCertFile))) {[m
[32m+[m[32m            return $caPath = $envCertFile;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // If SSL_CERT_DIR env variable points to a valid certificate/bundle, use that.[m
[32m+[m[32m        // This mimics how OpenSSL uses the SSL_CERT_FILE env variable.[m
[32m+[m[32m        $envCertDir = getenv('SSL_CERT_DIR');[m
[32m+[m[32m        if ($envCertDir && is_dir($envCertDir) && is_readable($envCertDir)) {[m
[32m+[m[32m            return $caPath = $envCertDir;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $configured = ini_get('openssl.cafile');[m
[32m+[m[32m        if ($configured && strlen($configured) > 0 && is_readable($configured) && validateCaFile(file_get_contents($configured))) {[m
[32m+[m[32m            return $caPath = $configured;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $configured = ini_get('openssl.capath');[m
[32m+[m[32m        if ($configured && is_dir($configured) && is_readable($configured)) {[m
[32m+[m[32m            return $caPath = $configured;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $caBundlePaths = array([m
[32m+[m[32m            '/etc/pki/tls/certs/ca-bundle.crt', // Fedora, RHEL, CentOS (ca-certificates package)[m
[32m+[m[32m            '/etc/ssl/certs/ca-certificates.crt', // Debian, Ubuntu, Gentoo, Arch Linux (ca-certificates package)[m
[32m+[m[32m            '/etc/ssl/ca-bundle.pem', // SUSE, openSUSE (ca-certificates package)[m
[32m+[m[32m            '/usr/local/share/certs/ca-root-nss.crt', // FreeBSD (ca_root_nss_package)[m
[32m+[m[32m            '/usr/ssl/certs/ca-bundle.crt', // Cygwin[m
[32m+[m[32m            '/opt/local/share/curl/curl-ca-bundle.crt', // OS X macports, curl-ca-bundle package[m
[32m+[m[32m            '/usr/local/share/curl/curl-ca-bundle.crt', // Default cURL CA bunde path (without --with-ca-bundle option)[m
[32m+[m[32m            '/usr/share/ssl/certs/ca-bundle.crt', // Really old RedHat?[m
[32m+[m[32m            '/etc/ssl/cert.pem', // OpenBSD[m
[32m+[m[32m            '/usr/local/etc/ssl/cert.pem', // FreeBSD 10.x[m
[32m+[m[32m            '/usr/local/etc/openssl/cert.pem', // OS X homebrew, openssl package[m
[32m+[m[32m            '/usr/local/etc/openssl@1.1/cert.pem', // OS X homebrew, openssl@1.1 package[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        foreach ($caBundlePaths as $caBundle) {[m
[32m+[m[32m            if (@is_readable($caBundle) && validateCaFile(file_get_contents($caBundle))) {[m
[32m+[m[32m                return $caPath = $caBundle;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        foreach ($caBundlePaths as $caBundle) {[m
[32m+[m[32m            $caBundle = dirname($caBundle);[m
[32m+[m[32m            if (is_dir($caBundle) && glob($caBundle.'/*')) {[m
[32m+[m[32m                return $caPath = $caBundle;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $caPath = false;[m
[32m+[m[32m    }[m
[32m+[m
