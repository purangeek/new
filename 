[1mdiff --git a/app/Http/Controllers/UserAuthController.php b/app/Http/Controllers/UserAuthController.php[m
[1mnew file mode 100644[m
[1mindex 0000000..2fab307[m
[1m--- /dev/null[m
[1m+++ b/app/Http/Controllers/UserAuthController.php[m
[36m@@ -0,0 +1,68 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace App\Http\Controllers;[m
[32m+[m
[32m+[m[32muse Illuminate\Http\Request;[m
[32m+[m[32muse Illuminate\Support\Facades\hash;[m
[32m+[m[32muse App\Models\Teacher;[m
[32m+[m
[32m+[m
[32m+[m[32mclass UserAuthController extends Controller[m
[32m+[m[32m{[m
[32m+[m[32m    function login(){[m
[32m+[m[32m        return view('auth.login');[m
[32m+[m[32m    }[m
[32m+[m[32m    function register(){[m
[32m+[m[32m        return view('auth.register');[m
[32m+[m[32m    }[m
[32m+[m[32m    function create(Request $request){[m
[32m+[m[32m        $request->validate([[m
[32m+[m[32m            'first_name' => 'required',[m
[32m+[m[32m            'email' => 'required|email|unique:teachers',[m
[32m+[m[32m            'password' => 'required|confirmed|min:5|max:12',[m
[32m+[m[32m            'inputAddress' => 'required',[m
[32m+[m[32m            'inputCountry' => 'required'[m
[32m+[m[32m        ]);[m
[32m+[m[32m        $teacher = new Teacher;[m
[32m+[m[32m        $teacher->first_name = $request->first_name;[m
[32m+[m[32m        $teacher->last_name = $request->last_name;[m
[32m+[m[32m        $teacher->gender = $request->gender;[m
[32m+[m[32m        $teacher->email = $request->email;[m
[32m+[m[32m        $teacher->password = Hash::make($request->password);[m
[32m+[m[32m        $teacher->address = $request->inputAddress;[m
[32m+[m[32m        $teacher->country = $request->inputCountry;[m
[32m+[m
[32m+[m[32m        $query = $teacher->save();[m
[32m+[m[32m        if($query){[m
[32m+[m[32m            return back()->with('success', 'You are registered Successfully');[m
[32m+[m[32m        }[m
[32m+[m[32m        else{[m
[32m+[m[32m            return back()->with('fail', 'something went wrong');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m    function check(Request $request){[m
[32m+[m[32m        $request->validate([[m
[32m+[m[32m            'email' => 'required',[m
[32m+[m[32m            'password' => 'required'[m
[32m+[m[32m        ]);[m
[32m+[m[41m    [m
[32m+[m[32m        // If form validate succesfull, process login[m
[32m+[m[32m        $teacher = Teacher::where('email', '=', $request->email)->first();[m
[32m+[m[32m        if($teacher){[m
[32m+[m[32m            if(Hash::check($request->password, $teacher->password)){[m
[32m+[m[32m                $request->session()->put('LoggedUser', $teacher->id);[m
[32m+[m[32m                return redirect('profile');[m
[32m+[m[32m            }else{[m
[32m+[m[32m                return back()->with('fail', 'Invalid Password');[m[41m    [m
[32m+[m[32m            }[m
[32m+[m[32m        }else{[m
[32m+[m[32m            return back()->with('fail', 'No Account found with this Email');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function profile(){[m
[32m+[m[32m        $user_id = session('LoggedUser');[m
[32m+[m[32m        $user = Teacher::Where('id', $user_id)->get()->first();[m
[32m+[m[32m        return view('admin.profile', ['LoggedTeacherInfo' => $user, '$x' => 1]);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
