[1mdiff --git a/composer-setup.php b/composer-setup.php[m
[1mnew file mode 100644[m
[1mindex 0000000..f3b1feb[m
[1m--- /dev/null[m
[1m+++ b/composer-setup.php[m
[36m@@ -0,0 +1,1733 @@[m
[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32m/*[m
[32m+[m[32m * This file is part of Composer.[m
[32m+[m[32m *[m
[32m+[m[32m * (c) Nils Adermann <naderman@naderman.de>[m
[32m+[m[32m *     Jordi Boggiano <j.boggiano@seld.be>[m
[32m+[m[32m *[m
[32m+[m[32m * For the full copyright and license information, please view the LICENSE[m
[32m+[m[32m * file that was distributed with this source code.[m
[32m+[m[32m */[m
[32m+[m
[32m+[m[32msetupEnvironment();[m
[32m+[m[32mprocess(is_array($argv) ? $argv : array());[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Initializes various values[m
[32m+[m[32m *[m
[32m+[m[32m * @throws RuntimeException If uopz extension prevents exit calls[m
[32m+[m[32m */[m
[32m+[m[32mfunction setupEnvironment()[m
[32m+[m[32m{[m
[32m+[m[32m    ini_set('display_errors', 1);[m
[32m+[m
[32m+[m[32m    if (extension_loaded('uopz') && !(ini_get('uopz.disable') || ini_get('uopz.exit'))) {[m
[32m+[m[32m        // uopz works at opcode level and disables exit calls[m
[32m+[m[32m        if (function_exists('uopz_allow_exit')) {[m
[32m+[m[32m            @uopz_allow_exit(true);[m
[32m+[m[32m        } else {[m
[32m+[m[32m            throw new RuntimeException('The uopz extension ignores exit calls and breaks this installer.');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $installer = 'ComposerInstaller';[m
[32m+[m
[32m+[m[32m    if (defined('PHP_WINDOWS_VERSION_MAJOR')) {[m
[32m+[m[32m        if ($version = getenv('COMPOSERSETUP')) {[m
[32m+[m[32m            $installer = sprintf('Composer-Setup.exe/%s', $version);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    define('COMPOSER_INSTALLER', $installer);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Processes the installer[m
[32m+[m[32m */[m
[32m+[m[32mfunction process($argv)[m
[32m+[m[32m{[m
[32m+[m[32m    // Determine ANSI output from --ansi and --no-ansi flags[m
[32m+[m[32m    setUseAnsi($argv);[m
[32m+[m
[32m+[m[32m    if (in_array('--help', $argv)) {[m
[32m+[m[32m        displayHelp();[m
[32m+[m[32m        exit(0);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $check      = in_array('--check', $argv);[m
[32m+[m[32m    $help       = in_array('--help', $argv);[m
[32m+[m[32m    $force      = in_array('--force', $argv);[m
[32m+[m[32m    $quiet      = in_array('--quiet', $argv);[m
[32m+[m[32m    $channel    = 'stable';[m
[32m+[m[32m    if (in_array('--snapshot', $argv)) {[m
[32m+[m[32m        $channel = 'snapshot';[m
[32m+[m[32m    } elseif (in_array('--preview', $argv)) {[m
[32m+[m[32m        $channel = 'preview';[m
[32m+[m[32m    } elseif (in_array('--1', $argv)) {[m
[32m+[m[32m        $channel = '1';[m
[32m+[m[32m    } elseif (in_array('--2', $argv)) {[m
[32m+[m[32m        $channel = '2';[m
[32m+[m[32m    }[m
[32m+[m[32m    $disableTls = in_array('--disable-tls', $argv);[m
[32m+[m[32m    $installDir = getOptValue('--install-dir', $argv, false);[m
[32m+[m[32m    $version    = getOptValue('--version', $argv, false);[m
[32m+[m[32m    $filename   = getOptValue('--filename', $argv, 'composer.phar');[m
[32m+[m[32m    $cafile     = getOptValue('--cafile', $argv, false);[m
[32m+[m
[32m+[m[32m    if (!checkParams($installDir, $version, $cafile)) {[m
[32m+[m[32m        exit(1);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $ok = checkPlatform($warnings, $quiet, $disableTls, true);[m
[32m+[m
[32m+[m[32m    if ($check) {[m
[32m+[m[32m        // Only show warnings if we haven't output any errors[m
[32m+[m[32m        if ($ok) {[m
[32m+[m[32m            showWarnings($warnings);[m
[32m+[m[32m            showSecurityWarning($disableTls);[m
[32m+[m[32m        }[m
[32m+[m[32m        exit($ok ? 0 : 1);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($ok || $force) {[m
[32m+[m[32m        $installer = new Installer($quiet, $disableTls, $cafile);[m
[32m+[m[32m        if ($installer->run($version, $installDir, $filename, $channel)) {[m
[32m+[m[32m            showWarnings($warnings);[m
[32m+[m[32m            showSecurityWarning($disableTls);[m
[32m+[m[32m            exit(0);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    exit(1);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Displays the help[m
[32m+[m[32m */[m
[32m+[m[32mfunction displayHelp()[m
[32m+[m[32m{[m
[32m+[m[32m    echo <<<EOF[m
[32m+[m[32mComposer Installer[m
[32m+[m[32m------------------[m
[32m+[m[32mOptions[m
[32m+[m[32m--help               this help[m
[32m+[m[32m--check              for checking environment only[m
[32m+[m[32m--force              forces the installation[m
[32m+[m[32m--ansi               force ANSI color output[m
[32m+[m[32m--no-ansi            disable ANSI color output[m
[32m+[m[32m--quiet              do not output unimportant messages[m
[32m+[m[32m--install-dir="..."  accepts a target installation directory[m
[32m+[m[32m--preview            install the latest version from the preview (alpha/beta/rc) channel instead of stable[m
[32m+[m[32m--snapshot           install the latest version from the snapshot (dev builds) channel instead of stable[m
[32m+[m[32m--1                  install the latest stable Composer 1.x version[m
[32m+[m[32m--2                  install the latest stable Composer 2.x version[m
[32m+[m[32m--version="..."      accepts a specific version to install instead of the latest[m
[32m+[m[32m--filename="..."     accepts a target filename (default: composer.phar)[m
[32m+[m[32m--disable-tls        disable SSL/TLS security for file downloads[m
[32m+[m[32m--cafile="..."       accepts a path to a Certificate Authority (CA) certificate file for SSL/TLS verification[m
[32m+[m
[32m+[m[32mEOF;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Sets the USE_ANSI define for colorizing output[m
[32m+[m[32m *[m
[32m+[m[32m * @param array $argv Command-line arguments[m
[32m+[m[32m */[m
[32m+[m[32mfunction setUseAnsi($argv)[m
[32m+[m[32m{[m
[32m+[m[32m    // --no-ansi wins over --ansi[m
[32m+[m[32m    if (in_array('--no-ansi', $argv)) {[m
[32m+[m[32m        define('USE_ANSI', false);[m
[32m+[m[32m    } elseif (in_array('--ansi', $argv)) {[m
[32m+[m[32m        define('USE_ANSI', true);[m
[32m+[m[32m    } else {[m
[32m+[m[32m        // On Windows, default to no ANSI, except in ANSICON and ConEmu.[m
[32m+[m[32m        // Everywhere else, default to ANSI if stdout is a terminal.[m
[32m+[m[32m        define([m
[32m+[m[32m            'USE_ANSI',[m
[32m+[m[32m            (DIRECTORY_SEPARATOR == '\\')[m
[32m+[m[32m                ? (false !== getenv('ANSICON') || 'ON' === getenv('ConEmuANSI'))[m
[32m+[m[32m                : (function_exists('posix_isatty') && posix_isatty(1))[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns the value of a command-line option[m
[32m+[m[32m *[m
[32m+[m[32m * @param string $opt The command-line option to check[m
[32m+[m[32m * @param array $argv Command-line arguments[m
[32m+[m[32m * @param mixed $default Default value to be returned[m
[32m+[m[32m *[m
[32m+[m[32m * @return mixed The command-line value or the default[m
[32m+[m[32m */[m
[32m+[m[32mfunction getOptValue($opt, $argv, $default)[m
[32m+[m[32m{[m
[32m+[m[32m    $optLength = strlen($opt);[m
[32m+[m
[32m+[m[32m    foreach ($argv as $key => $value) {[m
[32m+[m[32m        $next = $key + 1;[m
[32m+[m[32m        if (0 === strpos($value, $opt)) {[m
[32m+[m[32m            if ($optLength === strlen($value) && isset($argv[$next])) {[m
[32m+[m[32m                return trim($argv[$next]);[m
[32m+[m[32m            } else {[m
[32m+[m[32m                return trim(substr($value, $optLength + 1));[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return $default;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks that user-supplied params are valid[m
[32m+[m[32m *[m
[32m+[m[32m * @param mixed $installDir The required istallation directory[m
[32m+[m[32m * @param mixed $version The required composer version to install[m
[32m+[m[32m * @param mixed $cafile Certificate Authority file[m
[32m+[m[32m *[m
[32m+[m[32m * @return bool True if the supplied params are okay[m
[32m+[m[32m */[m
[32m+[m[32mfunction checkParams($installDir, $version, $cafile)[m
[32m+[m[32m{[m
[32m+[m[32m    $result = true;[m
[32m+[m
[32m+[m[32m    if (false !== $installDir && !is_dir($installDir)) {[m
[32m+[m[32m        out("The defined install dir ({$installDir}) does not exist.", 'info');[m
[32m+[m[32m        $result = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (false !== $version && 1 !== preg_match('/^\d+\.\d+\.\d+(\-(alpha|beta|RC)\d*)*$/', $version)) {[m
[32m+[m[32m        out("The defined install version ({$version}) does not match release pattern.", 'info');[m
[32m+[m[32m        $result = false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (false !== $cafile && (!file_exists($cafile) || !is_readable($cafile))) {[m
[32m+[m[32m        out("The defined Certificate Authority (CA) cert file ({$cafile}) does not exist or is not readable.", 'info');[m
[32m+[m[32m        $result = false;[m
[32m+[m[32m    }[m
[32m+[m[32m    return $result;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks the platform for possible issues running Composer[m
[32m+[m[32m *[m
[32m+[m[32m * Errors are written to the output, warnings are saved for later display.[m
[32m+[m[32m *[m
[32m+[m[32m * @param array $warnings Populated by method, to be shown later[m
[32m+[m[32m * @param bool $quiet Quiet mode[m
[32m+[m[32m * @param bool $disableTls Bypass tls[m
[32m+[m[32m * @param bool $install If we are installing, rather than diagnosing[m
[32m+[m[32m *[m
[32m+[m[32m * @return bool True if there are no errors[m
[32m+[m[32m */[m
[32m+[m[32mfunction checkPlatform(&$warnings, $quiet, $disableTls, $install)[m
[32m+[m[32m{[m
[32m+[m[32m    getPlatformIssues($errors, $warnings, $install);[m
[32m+[m
[32m+[m[32m    // Make openssl warning an error if tls has not been specifically disabled[m
[32m+[m[32m    if (isset($warnings['openssl']) && !$disableTls) {[m
[32m+[m[32m        $errors['openssl'] = $warnings['openssl'];[m
[32m+[m[32m        unset($warnings['openssl']);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!empty($errors)) {[m
[32m+[m[32m        // Composer-Setup.exe uses "Some settings" to flag platform errors[m
[32m+[m[32m        out('Some settings on your machine make Composer unable to work properly.', 'error');[m
[32m+[m[32m        out('Make sure that you fix the issues listed below and run this script again:', 'error');[m
[32m+[m[32m        outputIssues($errors);[m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (empty($warnings) && !$quiet) {[m
[32m+[m[32m        out('All settings correct for using Composer', 'success');[m
[32m+[m[32m    }[m
[32m+[m[32m    return true;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Checks platform configuration for common incompatibility issues[m
[32m+[m[32m *[m
[32m+[m[32m * @param array $errors Populated by method[m
[32m+[m[32m * @param array $warnings Populated by method[m
[32m+[m[32m * @param bool $install If we are installing, rather than diagnosing[m
[32m+[m[32m *[m
[32m+[m[32m * @return bool If any errors or warnings have been found[m
[32m+[m[32m */[m
[32m+[m[32mfunction getPlatformIssues(&$errors, &$warnings, $install)[m
[32m+[m[32m{[m
[32m+[m[32m    $errors = array();[m
[32m+[m[32m    $warnings = array();[m
[32m+[m
[32m+[m[32m    if ($iniPath = php_ini_loaded_file()) {[m
[32m+[m[32m        $iniMessage = PHP_EOL.'The php.ini used by your command-line PHP is: ' . $iniPath;[m
[32m+[m[32m    } else {[m
[32m+[m[32m        $iniMessage = PHP_EOL.'A php.ini file does not exist. You will have to create one.';[m
[32m+[m[32m    }[m
[32m+[m[32m    $iniMessage .= PHP_EOL.'If you can not modify the ini file, you can also run `php -d option=value` to modify ini values on the fly. You can use -d multiple times.';[m
[32m+[m
[32m+[m[32m    if (ini_get('detect_unicode')) {[m
[32m+[m[32m        $errors['unicode'] = array([m
[32m+[m[32m            'The detect_unicode setting must be disabled.',[m
[32m+[m[32m            'Add the following to the end of your `php.ini`:',[m
[32m+[m[32m            '    detect_unicode = Off',[m
[32m+[m[32m            $iniMessage[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (extension_loaded('suhosin')) {[m
[32m+[m[32m        $suhosin = ini_get('suhosin.executor.include.whitelist');[m
[32m+[m[32m        $suhosinBlacklist = ini_get('suhosin.executor.include.blacklist');[m
[32m+[m[32m        if (false === stripos($suhosin, 'phar') && (!$suhosinBlacklist || false !== stripos($suhosinBlacklist, 'phar'))) {[m
[32m+[m[32m            $errors['suhosin'] = array([m
[32m+[m[32m                'The suhosin.executor.include.whitelist setting is incorrect.',[m
[32m+[m[32m                'Add the following to the end of your `php.ini` or suhosin.ini (Example path [for Debian]: /etc/php5/cli/conf.d/suhosin.ini):',[m
[32m+[m[32m                '    suhosin.executor.include.whitelist = phar '.$suhosin,[m
[32m+[m[32m                $iniMessage[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!function_exists('json_decode')) {[m
[32m+[m[32m        $errors['json'] = array([m
[32m+[m[32m            'The json extension is missing.',[m
[32m+[m[32m            'Install it or recompile php without --disable-json'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!extension_loaded('Phar')) {[m
[32m+[m[32m        $errors['phar'] = array([m
[32m+[m[32m            'The phar extension is missing.',[m
[32m+[m[32m            'Install it or recompile php without --disable-phar'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!extension_loaded('filter')) {[m
[32m+[m[32m        $errors['filter'] = array([m
[32m+[m[32m            'The filter extension is missing.',[m
[32m+[m[32m            'Install it or recompile php without --disable-filter'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!extension_loaded('hash')) {[m
[32m+[m[32m        $errors['hash'] = array([m
[32m+[m[32m            'The hash extension is missing.',[m
[32m+[m[32m            'Install it or recompile php without --disable-hash'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!extension_loaded('iconv') && !extension_loaded('mbstring')) {[m
[32m+[m[32m        $errors['iconv_mbstring'] = array([m
[32m+[m[32m            'The iconv OR mbstring extension is required and both are missing.',[m
[32m+[m[32m            'Install either of them or recompile php without --disable-iconv'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!ini_get('allow_url_fopen')) {[m
[32m+[m[32m        $errors['allow_url_fopen'] = array([m
[32m+[m[32m            'The allow_url_fopen setting is incorrect.',[m
[32m+[m[32m            'Add the following to the end of your `php.ini`:',[m
[32m+[m[32m            '    allow_url_fopen = On',[m
[32m+[m[32m            $iniMessage[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (extension_loaded('ionCube Loader') && ioncube_loader_iversion() < 40009) {[m
[32m+[m[32m        $ioncube = ioncube_loader_version();[m
[32m+[m[32m        $errors['ioncube'] = array([m
[32m+[m[32m            'Your ionCube Loader extension ('.$ioncube.') is incompatible with Phar files.',[m
[32m+[m[32m            'Upgrade to ionCube 4.0.9 or higher or remove this line (path may be different) from your `php.ini` to disable it:',[m
[32m+[m[32m            '    zend_extension = /usr/lib/php5/20090626+lfs/ioncube_loader_lin_5.3.so',[m
[32m+[m[32m            $iniMessage[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (version_compare(PHP_VERSION, '5.3.2', '<')) {[m
[32m+[m[32m        $errors['php'] = array([m
[32m+[m[32m            'Your PHP ('.PHP_VERSION.') is too old, you must upgrade to PHP 5.3.2 or higher.'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (version_compare(PHP_VERSION, '5.3.4', '<')) {[m
[32m+[m[32m        $warnings['php'] = array([m
[32m+[m[32m            'Your PHP ('.PHP_VERSION.') is quite old, upgrading to PHP 5.3.4 or higher is recommended.',[m
[32m+[m[32m            'Composer works with 5.3.2+ for most people, but there might be edge case issues.'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!extension_loaded('openssl')) {[m
[32m+[m[32m        $warnings['openssl'] = array([m
[32m+[m[32m            'The openssl extension is missing, which means that secure HTTPS transfers are impossible.',[m
[32m+[m[32m            'If possible you should enable it or recompile php with --with-openssl'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (extension_loaded('openssl') && OPENSSL_VERSION_NUMBER < 0x1000100f) {[m
[32m+[m[32m        // Attempt to parse version number out, fallback to whole string value.[m
[32m+[m[32m        $opensslVersion = trim(strstr(OPENSSL_VERSION_TEXT, ' '));[m
[32m+[m[32m        $opensslVersion = substr($opensslVersion, 0, strpos($opensslVersion, ' '));[m
[32m+[m[32m        $opensslVersion = $opensslVersion ? $opensslVersion : OPENSSL_VERSION_TEXT;[m
[32m+[m
[32m+[m[32m        $warnings['openssl_version'] = array([m
[32m+[m[32m            'The OpenSSL library ('.$opensslVersion.') used by PHP does not support TLSv1.2 or TLSv1.1.',[m
[32m+[m[32m            'If possible you should upgrade OpenSSL to version 1.0.1 or above.'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!defined('HHVM_VERSION') && !extension_loaded('apcu') && ini_get('apc.enable_cli')) {[m
[32m+[m[32m        $warnings['apc_cli'] = array([m
[32m+[m[32m            'The apc.enable_cli setting is incorrect.',[m
[32m+[m[32m            'Add the following to the end of your `php.ini`:',[m
[32m+[m[32m            '    apc.enable_cli = Off',[m
[32m+[m[32m            $iniMessage[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!$install && extension_loaded('xdebug')) {[m
[32m+[m[32m        $warnings['xdebug_loaded'] = array([m
[32m+[m[32m            'The xdebug extension is loaded, this can slow down Composer a little.',[m
[32m+[m[32m            'Disabling it when using Composer is recommended.'[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (ini_get('xdebug.profiler_enabled')) {[m
[32m+[m[32m            $warnings['xdebug_profile'] = array([m
[32m+[m[32m                'The xdebug.profiler_enabled setting is enabled, this can slow down Composer a lot.',[m
[32m+[m[32m                'Add the following to the end of your `php.ini` to disable it:',[m
[32m+[m[32m                '    xdebug.profiler_enabled = 0',[m
[32m+[m[32m                $iniMessage[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (!extension_loaded('zlib')) {[m
[32m+[m[32m        $warnings['zlib'] = array([m
[32m+[m[32m            'The zlib extension is not loaded, this can slow down Composer a lot.',[m
[32m+[m[32m            'If possible, install it or recompile php with --with-zlib',[m
[32m+[m[32m            $iniMessage[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (defined('PHP_WINDOWS_VERSION_BUILD')[m
[32m+[m[32m        && (version_compare(PHP_VERSION, '7.2.23', '<')[m
[32m+[m[32m        || (version_compare(PHP_VERSION, '7.3.0', '>=')[m
[32m+[m[32m        && version_compare(PHP_VERSION, '7.3.10', '<')))) {[m
[32m+[m[32m        $warnings['onedrive'] = array([m
[32m+[m[32m            'The Windows OneDrive folder is not supported on PHP versions below 7.2.23 and 7.3.10.',[m
[32m+[m[32m            'Upgrade your PHP ('.PHP_VERSION.') to use this location with Composer.'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (extension_loaded('uopz') && !(ini_get('uopz.disable') || ini_get('uopz.exit'))) {[m
[32m+[m[32m        $warnings['uopz'] = array([m
[32m+[m[32m            'The uopz extension ignores exit calls and may not work with all Composer commands.',[m
[32m+[m[32m            'Disabling it when using Composer is recommended.'[m
[32m+[m[32m        );[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    ob_start();[m
[32m+[m[32m    phpinfo(INFO_GENERAL);[m
[32m+[m[32m    $phpinfo = ob_get_clean();[m
[32m+[m[32m    if (preg_match('{Configure Command(?: *</td><td class="v">| *=> *)(.*?)(?:</td>|$)}m', $phpinfo, $match)) {[m
[32m+[m[32m        $configure = $match[1];[m
[32m+[m
[32m+[m[32m        if (false !== strpos($configure, '--enable-sigchild')) {[m
[32m+[m[32m            $warnings['sigchild'] = array([m
[32m+[m[32m                'PHP was compiled with --enable-sigchild which can cause issues on some platforms.',[m
[32m+[m[32m                'Recompile it without this flag if possible, see also:',[m
[32m+[m[32m                '    https://bugs.php.net/bug.php?id=22999'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (false !== strpos($configure, '--with-curlwrappers')) {[m
[32m+[m[32m            $warnings['curlwrappers'] = array([m
[32m+[m[32m                'PHP was compiled with --with-curlwrappers which will cause issues with HTTP authentication and GitHub.',[m
[32m+[m[32m                'Recompile it without this flag if possible'[m
[32m+[m[32m            );[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // Stringify the message arrays[m
[32m+[m[32m    foreach ($errors as $key => $value) {[m
[32m+[m[32m        $errors[$key] = PHP_EOL.implode(PHP_EOL, $value);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    foreach ($warnings as $key => $value) {[m
[32m+[m[32m        $warnings[$key] = PHP_EOL.implode(PHP_EOL, $value);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return !empty($errors) || !empty($warnings);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Outputs an array of issues[m
[32m+[m[32m *[m
[32m+[m[32m * @param array $issues[m
[32m+[m[32m */[m
[32m+[m[32mfunction outputIssues($issues)[m
[32m+[m[32m{[m
[32m+[m[32m    foreach ($issues as $issue) {[m
[32m+[m[32m        out($issue, 'info');[m
[32m+[m[32m    }[m
[32m+[m[32m    out('');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Outputs any warnings found[m
[32m+[m[32m *[m
[32m+[m[32m * @param array $warnings[m
[32m+[m[32m */[m
[32m+[m[32mfunction showWarnings($warnings)[m
[32m+[m[32m{[m
[32m+[m[32m    if (!empty($warnings)) {[m
[32m+[m[32m        out('Some settings on your machine may cause stability issues with Composer.', 'error');[m
[32m+[m[32m        out('If you encounter issues, try to change the following:', 'error');[m
[32m+[m[32m        outputIssues($warnings);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Outputs an end of process warning if tls has been bypassed[m
[32m+[m[32m *[m
[32m+[m[32m * @param bool $disableTls Bypass tls[m
[32m+[m[32m */[m
[32m+[m[32mfunction showSecurityWarning($disableTls)[m
[32m+[m[32m{[m
[32m+[m[32m    if ($disableTls) {[m
[32m+[m[32m        out('You have instructed the Installer not to enforce SSL/TLS security on remote HTTPS requests.', 'info');[m
[32m+[m[32m        out('This will leave all downloads during installation vulnerable to Man-In-The-Middle (MITM) attacks', 'info');[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * colorize output[m
[32m+[m[32m */[m
[32m+[m[32mfunction out($text, $color = null, $newLine = true)[m
[32m+[m[32m{[m
[32m+[m[32m    $styles = array([m
[32m+[m[32m        'success' => "\033[0;32m%s\033[0m",[m
[32m+[m[32m        'error' => "\033[31;31m%s\033[0m",[m
[32m+[m[32m        'info' => "\033[33;33m%s\033[0m"[m
[32m+[m[32m    );[m
[32m+[m
[32m+[m[32m    $format = '%s';[m
[32m+[m
[32m+[m[32m    if (isset($styles[$color]) && USE_ANSI) {[m
[32m+[m[32m        $format = $styles[$color];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if ($newLine) {[m
[32m+[m[32m        $format .= PHP_EOL;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    printf($format, $text);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns the system-dependent Composer home location, which may not exist[m
[32m+[m[32m *[m
[32m+[m[32m * @return string[m
[32m+[m[32m */[m
[32m+[m[32mfunction getHomeDir()[m
[32m+[m[32m{[m
[32m+[m[32m    $home = getenv('COMPOSER_HOME');[m
[32m+[m[32m    if ($home) {[m
[32m+[m[32m        return $home;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $userDir = getUserDir();[m
[32m+[m
[32m+[m[32m    if (defined('PHP_WINDOWS_VERSION_MAJOR')) {[m
[32m+[m[32m        return $userDir.'/Composer';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $dirs = array();[m
[32m+[m
[32m+[m[32m    if (useXdg()) {[m
[32m+[m[32m        // XDG Base Directory Specifications[m
[32m+[m[32m        $xdgConfig = getenv('XDG_CONFIG_HOME');[m
[32m+[m[32m        if (!$xdgConfig) {[m
[32m+[m[32m            $xdgConfig = $userDir . '/.config';[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $dirs[] = $xdgConfig . '/composer';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    $dirs[] = $userDir . '/.composer';[m
[32m+[m
[32m+[m[32m    // select first dir which exists of: $XDG_CONFIG_HOME/composer or ~/.composer[m
[32m+[m[32m    foreach ($dirs as $dir) {[m
[32m+[m[32m        if (is_dir($dir)) {[m
[32m+[m[32m            return $dir;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    // if none exists, we default to first defined one (XDG one if system uses it, or ~/.composer otherwise)[m
[32m+[m[32m    return $dirs[0];[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * Returns the location of the user directory from the environment[m
[32m+[m[32m * @throws RuntimeException If the environment value does not exists[m
[32m+[m[32m *[m
[32m+[m[32m * @return string[m
[32m+[m[32m */[m
[32m+[m[32mfunction getUserDir()[m
[32m+[m[32m{[m
[32m+[m[32m    $userEnv = defined('PHP_WINDOWS_VERSION_MAJOR') ? 'APPDATA' : 'HOME';[m
[32m+[m[32m    $userDir = getenv($userEnv);[m
[32m+[m
[32m+[m[32m    if (!$userDir) {[m
[32m+[m[32m        throw new RuntimeException('The '.$userEnv.' or COMPOSER_HOME environment variable must be set for composer to run correctly');[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return rtrim(strtr($userDir, '\\', '/'), '/');[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32m/**[m
[32m+[m[32m * @return bool[m
[32m+[m[32m */[m
[32m+[m[32mfunction useXdg()[m
[32m+[m[32m{[m
[32m+[m[32m    foreach (array_keys($_SERVER) as $key) {[m
[32m+[m[32m        if (strpos($key, 'XDG_') === 0) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    if (is_dir('/etc/xdg')) {[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return false;[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mfunction validateCaFile($contents)[m
[32m+[m[32m{[m
[32m+[m[32m    // assume the CA is valid if php is vulnerable to[m
[32m+[m[32m    // https://www.sektioneins.de/advisories/advisory-012013-php-openssl_x509_parse-memory-corruption-vulnerability.html[m
[32m+[m[32m    if ([m
[32m+[m[32m        PHP_VERSION_ID <= 50327[m
[32m+[m[32m        || (PHP_VERSION_ID >= 50400 && PHP_VERSION_ID < 50422)[m
[32m+[m[32m        || (PHP_VERSION_ID >= 50500 && PHP_VERSION_ID < 50506)[m
[32m+[m[32m    ) {[m
[32m+[m[32m        return !empty($contents);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    return (bool) openssl_x509_parse($contents);[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mclass Installer[m
[32m+[m[32m{[m
[32m+[m[32m    private $quiet;[m
[32m+[m[32m    private $disableTls;[m
[32m+[m[32m    private $cafile;[m
[32m+[m[32m    private $displayPath;[m
[32m+[m[32m    private $target;[m
[32m+[m[32m    private $tmpFile;[m
[32m+[m[32m    private $tmpCafile;[m
[32m+[m[32m    private $baseUrl;[m
[32m+[m[32m    private $algo;[m
[32m+[m[32m    private $errHandler;[m
[32m+[m[32m    private $httpClient;[m
[32m+[m[32m    private $pubKeys = array();[m
[32m+[m[32m    private $installs = array();[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Constructor - must not do anything that throws an exception[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param bool $quiet Quiet mode[m
[32m+[m[32m     * @param bool $disableTls Bypass tls[m
[32m+[m[32m     * @param mixed $cafile Path to CA bundle, or false[m
[32m+[m[32m     */[m
[32m+[m[32m    public function __construct($quiet, $disableTls, $caFile)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (($this->quiet = $quiet)) {[m
[32m+[m[32m            ob_start();[m
[32m+[m[32m        }[m
[32m+[m[32m        $this->disableTls = $disableTls;[m
[32m+[m[32m        $this->cafile = $caFile;[m
[32m+[m[32m        $this->errHandler = new ErrorHandler();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Runs the installer[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param mixed $version Specific version to install, or false[m
[32m+[m[32m     * @param mixed $installDir Specific installation directory, or false[m
[32m+[m[32m     * @param string $filename Specific filename to save to, or composer.phar[m
[32m+[m[32m     * @param string $channel Specific version channel to use[m
[32m+[m[32m     * @throws Exception If anything other than a RuntimeException is caught[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the installation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    public function run($version, $installDir, $filename, $channel)[m
[32m+[m[32m    {[m
[32m+[m[32m        try {[m
[32m+[m[32m            $this->initTargets($installDir, $filename);[m
[32m+[m[32m            $this->initTls();[m
[32m+[m[32m            $this->httpClient = new HttpClient($this->disableTls, $this->cafile);[m
[32m+[m[32m            $result = $this->install($version, $channel);[m
[32m+[m
[32m+[m[32m            // in case --1 or --2 is passed, we leave the default channel for next self-update to stable[m
[32m+[m[32m            if (is_numeric($channel)) {[m
[32m+[m[32m                $channel = 'stable';[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if ($result && $channel !== 'stable' && !$version && defined('PHP_BINARY')) {[m
[32m+[m[32m                $null = (defined('PHP_WINDOWS_VERSION_MAJOR') ? 'NUL' : '/dev/null');[m
[32m+[m[32m                @exec(escapeshellarg(PHP_BINARY) .' '.escapeshellarg($this->target).' self-update --'.$channel.' --set-channel-only -q > '.$null.' 2> '.$null, $output);[m
[32m+[m[32m            }[m
[32m+[m[32m        } catch (Exception $e) {[m
[32m+[m[32m            $result = false;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Always clean up[m
[32m+[m[32m        $this->cleanUp($result);[m
[32m+[m
[32m+[m[32m        if (isset($e)) {[m
[32m+[m[32m            // Rethrow anything that is not a RuntimeException[m
[32m+[m[32m            if (!$e instanceof RuntimeException) {[m
[32m+[m[32m                throw $e;[m
[32m+[m[32m            }[m
[32m+[m[32m            out($e->getMessage(), 'error');[m
[32m+[m[32m        }[m
[32m+[m[32m        return $result;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Initialization methods to set the required filenames and composer url[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param mixed $installDir Specific installation directory, or false[m
[32m+[m[32m     * @param string $filename Specific filename to save to, or composer.phar[m
[32m+[m[32m     * @throws RuntimeException If the installation directory is not writable[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function initTargets($installDir, $filename)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->displayPath = ($installDir ? rtrim($installDir, '/').'/' : '').$filename;[m
[32m+[m[32m        $installDir = $installDir ? realpath($installDir) : getcwd();[m
[32m+[m
[32m+[m[32m        if (!is_writeable($installDir)) {[m
[32m+[m[32m            throw new RuntimeException('The installation directory "'.$installDir.'" is not writable');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->target = $installDir.DIRECTORY_SEPARATOR.$filename;[m
[32m+[m[32m        $this->tmpFile = $installDir.DIRECTORY_SEPARATOR.basename($this->target, '.phar').'-temp.phar';[m
[32m+[m
[32m+[m[32m        $uriScheme = $this->disableTls ? 'http' : 'https';[m
[32m+[m[32m        $this->baseUrl = $uriScheme.'://getcomposer.org';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * A wrapper around methods to check tls and write public keys[m
[32m+[m[32m     * @throws RuntimeException If SHA384 is not supported[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function initTls()[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->disableTls) {[m
[32m+[m[32m            return;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!in_array('sha384', array_map('strtolower', openssl_get_md_methods()))) {[m
[32m+[m[32m            throw new RuntimeException('SHA384 is not supported by your openssl extension');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->algo = defined('OPENSSL_ALGO_SHA384') ? OPENSSL_ALGO_SHA384 : 'SHA384';[m
[32m+[m[32m        $home = $this->getComposerHome();[m
[32m+[m
[32m+[m[32m        $this->pubKeys = array([m
[32m+[m[32m            'dev' => $this->installKey(self::getPKDev(), $home, 'keys.dev.pub'),[m
[32m+[m[32m            'tags' => $this->installKey(self::getPKTags(), $home, 'keys.tags.pub')[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        if (empty($this->cafile) && !HttpClient::getSystemCaRootBundlePath()) {[m
[32m+[m[32m            $this->cafile = $this->tmpCafile = $this->installKey(HttpClient::getPackagedCaFile(), $home, 'cacert-temp.pem');[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns the Composer home directory, creating it if required[m
[32m+[m[32m     * @throws RuntimeException If the directory cannot be created[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function getComposerHome()[m
[32m+[m[32m    {[m
[32m+[m[32m        $home = getHomeDir();[m
[32m+[m
[32m+[m[32m        if (!is_dir($home)) {[m
[32m+[m[32m            $this->errHandler->start();[m
[32m+[m
[32m+[m[32m            if (!mkdir($home, 0777, true)) {[m
[32m+[m[32m                throw new RuntimeException(sprintf([m
[32m+[m[32m                    'Unable to create Composer home directory "%s": %s',[m
[32m+[m[32m                    $home,[m
[32m+[m[32m                    $this->errHandler->message[m
[32m+[m[32m                ));[m
[32m+[m[32m            }[m
[32m+[m[32m            $this->installs[] = $home;[m
[32m+[m[32m            $this->errHandler->stop();[m
[32m+[m[32m        }[m
[32m+[m[32m        return $home;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Writes public key data to disc[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $data The public key(s) in pem format[m
[32m+[m[32m     * @param string $path The directory to write to[m
[32m+[m[32m     * @param string $filename The name of the file[m
[32m+[m[32m     * @throws RuntimeException If the file cannot be written[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string The path to the saved data[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function installKey($data, $path, $filename)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->errHandler->start();[m
[32m+[m
[32m+[m[32m        $target = $path.DIRECTORY_SEPARATOR.$filename;[m
[32m+[m[32m        $installed = file_exists($target);[m
[32m+[m[32m        $write = file_put_contents($target, $data, LOCK_EX);[m
[32m+[m[32m        @chmod($target, 0644);[m
[32m+[m
[32m+[m[32m        $this->errHandler->stop();[m
[32m+[m
[32m+[m[32m        if (!$write) {[m
[32m+[m[32m            throw new RuntimeException(sprintf('Unable to write %s to: %s', $filename, $path));[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!$installed) {[m
[32m+[m[32m            $this->installs[] = $target;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $target;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * The main install function[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param mixed $version Specific version to install, or false[m
[32m+[m[32m     * @param string $channel Version channel to use[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the installation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function install($version, $channel)[m
[32m+[m[32m    {[m
[32m+[m[32m        $retries = 3;[m
[32m+[m[32m        $result = false;[m
[32m+[m[32m        $infoMsg = 'Downloading...';[m
[32m+[m[32m        $infoType = 'info';[m
[32m+[m
[32m+[m[32m        while ($retries--) {[m
[32m+[m[32m            if (!$this->quiet) {[m
[32m+[m[32m                out($infoMsg, $infoType);[m
[32m+[m[32m                $infoMsg = 'Retrying...';[m
[32m+[m[32m                $infoType = 'error';[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!$this->getVersion($channel, $version, $url, $error)) {[m
[32m+[m[32m                out($error, 'error');[m
[32m+[m[32m                continue;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!$this->downloadToTmp($url, $signature, $error)) {[m
[32m+[m[32m                out($error, 'error');[m
[32m+[m[32m                continue;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if (!$this->verifyAndSave($version, $signature, $error)) {[m
[32m+[m[32m                out($error, 'error');[m
[32m+[m[32m                continue;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            $result = true;[m
[32m+[m[32m            break;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!$this->quiet) {[m
[32m+[m[32m            if ($result) {[m
[32m+[m[32m                out(PHP_EOL."Composer (version {$version}) successfully installed to: {$this->target}", 'success');[m
[32m+[m[32m                out("Use it: php {$this->displayPath}", 'info');[m
[32m+[m[32m                out('');[m
[32m+[m[32m            } else {[m
[32m+[m[32m                out('The download failed repeatedly, aborting.', 'error');[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        return $result;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Sets the version url, downloading version data if required[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $channel Version channel to use[m
[32m+[m[32m     * @param false|string $version Version to install, or set by method[m
[32m+[m[32m     * @param null|string $url The versioned url, set by method[m
[32m+[m[32m     * @param null|string $error Set by method on failure[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the operation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function getVersion($channel, &$version, &$url, &$error)[m
[32m+[m[32m    {[m
[32m+[m[32m        $error = '';[m
[32m+[m
[32m+[m[32m        if ($version) {[m
[32m+[m[32m            if (empty($url)) {[m
[32m+[m[32m                $url = $this->baseUrl."/download/{$version}/composer.phar";[m
[32m+[m[32m            }[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->errHandler->start();[m
[32m+[m
[32m+[m[32m        if ($this->downloadVersionData($data, $error)) {[m
[32m+[m[32m            $this->parseVersionData($data, $channel, $version, $url);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        $this->errHandler->stop();[m
[32m+[m[32m        return empty($error);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Downloads and json-decodes version data[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param null|array $data Downloaded version data, set by method[m
[32m+[m[32m     * @param null|string $error Set by method on failure[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the operation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function downloadVersionData(&$data, &$error)[m
[32m+[m[32m    {[m
[32m+[m[32m        $url = $this->baseUrl.'/versions';[m
[32m+[m[32m        $errFmt = 'The "%s" file could not be %s: %s';[m
[32m+[m
[32m+[m[32m        if (!$json = $this->httpClient->get($url)) {[m
[32m+[m[32m            $error = sprintf($errFmt, $url, 'downloaded', $this->errHandler->message);[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!$data = json_decode($json, true)) {[m
[32m+[m[32m            $error = sprintf($errFmt, $url, 'json-decoded', $this->getJsonError());[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m[32m        return true;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * A wrapper around the methods needed to download and save the phar[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $url The versioned download url[m
[32m+[m[32m     * @param null|string $signature Set by method on successful download[m
[32m+[m[32m     * @param null|string $error Set by method on failure[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the operation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function downloadToTmp($url, &$signature, &$error)[m
[32m+[m[32m    {[m
[32m+[m[32m        $error = '';[m
[32m+[m[32m        $errFmt = 'The "%s" file could not be downloaded: %s';[m
[32m+[m[32m        $sigUrl = $url.'.sig';[m
[32m+[m[32m        $this->errHandler->start();[m
[32m+[m
[32m+[m[32m        if (!$fh = fopen($this->tmpFile, 'w')) {[m
[32m+[m[32m            $error = sprintf('Could not create file "%s": %s', $this->tmpFile, $this->errHandler->message);[m
[32m+[m
[32m+[m[32m        } elseif (!$this->getSignature($sigUrl, $signature)) {[m
[32m+[m[32m            $error = sprintf($errFmt, $sigUrl, $this->errHandler->message);[m
[32m+[m
[32m+[m[32m        } elseif (!fwrite($fh, $this->httpClient->get($url))) {[m
[32m+[m[32m            $error = sprintf($errFmt, $url, $this->errHandler->message);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (is_resource($fh)) {[m
[32m+[m[32m            fclose($fh);[m
[32m+[m[32m        }[m
[32m+[m[32m        $this->errHandler->stop();[m
[32m+[m[32m        return empty($error);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Verifies the downloaded file and saves it to the target location[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $version The composer version downloaded[m
[32m+[m[32m     * @param string $signature The digital signature to check[m
[32m+[m[32m     * @param null|string $error Set by method on failure[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the operation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function verifyAndSave($version, $signature, &$error)[m
[32m+[m[32m    {[m
[32m+[m[32m        $error = '';[m
[32m+[m
[32m+[m[32m        if (!$this->validatePhar($this->tmpFile, $pharError)) {[m
[32m+[m[32m            $error = 'The download is corrupt: '.$pharError;[m
[32m+[m
[32m+[m[32m        } elseif (!$this->verifySignature($version, $signature, $this->tmpFile)) {[m
[32m+[m[32m            $error = 'Signature mismatch, could not verify the phar file integrity';[m
[32m+[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $this->errHandler->start();[m
[32m+[m
[32m+[m[32m            if (!rename($this->tmpFile, $this->target)) {[m
[32m+[m[32m                $error = sprintf('Could not write to file "%s": %s', $this->target, $this->errHandler->message);[m
[32m+[m[32m            }[m
[32m+[m[32m            chmod($this->target, 0755);[m
[32m+[m[32m            $this->errHandler->stop();[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return empty($error);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Parses an array of version data to match the required channel[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param array $data Downloaded version data[m
[32m+[m[32m     * @param mixed $channel Version channel to use[m
[32m+[m[32m     * @param false|string $version Set by method[m
[32m+[m[32m     * @param mixed $url The versioned url, set by method[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function parseVersionData(array $data, $channel, &$version, &$url)[m
[32m+[m[32m    {[m
[32m+[m[32m        foreach ($data[$channel] as $candidate) {[m
[32m+[m[32m            if ($candidate['min-php'] <= PHP_VERSION_ID) {[m
[32m+[m[32m                $version = $candidate['version'];[m
[32m+[m[32m                $url = $this->baseUrl.$candidate['path'];[m
[32m+[m[32m                break;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!$version) {[m
[32m+[m[32m            $error = sprintf([m
[32m+[m[32m                'None of the %d %s version(s) of Composer matches your PHP version (%s / ID: %d)',[m
[32m+[m[32m                count($data[$channel]),[m
[32m+[m[32m                $channel,[m
[32m+[m[32m                PHP_VERSION,[m
[32m+[m[32m                PHP_VERSION_ID[m
[32m+[m[32m            );[m
[32m+[m[32m            throw new RuntimeException($error);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Downloads the digital signature of required phar file[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $url The signature url[m
[32m+[m[32m     * @param null|string $signature Set by method on success[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the download succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function getSignature($url, &$signature)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$result = $this->disableTls) {[m
[32m+[m[32m            $signature = $this->httpClient->get($url);[m
[32m+[m
[32m+[m[32m            if ($signature) {[m
[32m+[m[32m                $signature = json_decode($signature, true);[m
[32m+[m[32m                $signature = base64_decode($signature['sha384']);[m
[32m+[m[32m                $result = true;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $result;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Verifies the signature of the downloaded phar[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $version The composer versione[m
[32m+[m[32m     * @param string $signature The downloaded digital signature[m
[32m+[m[32m     * @param string $file The temp phar file[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the operation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function verifySignature($version, $signature, $file)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$result = $this->disableTls) {[m
[32m+[m[32m            $path = preg_match('{^[0-9a-f]{40}$}', $version) ? $this->pubKeys['dev'] : $this->pubKeys['tags'];[m
[32m+[m[32m            $pubkeyid = openssl_pkey_get_public('file://'.$path);[m
[32m+[m
[32m+[m[32m            $result = 1 === openssl_verify([m
[32m+[m[32m                file_get_contents($file),[m
[32m+[m[32m                $signature,[m
[32m+[m[32m                $pubkeyid,[m
[32m+[m[32m                $this->algo[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            // PHP 8 automatically frees the key instance and deprecates the function[m
[32m+[m[32m            if (PHP_VERSION_ID < 80000) {[m
[32m+[m[32m                openssl_free_key($pubkeyid);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $result;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Validates the downloaded phar file[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $pharFile The temp phar file[m
[32m+[m[32m     * @param null|string $error Set by method on failure[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool If the operation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function validatePhar($pharFile, &$error)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (ini_get('phar.readonly')) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        try {[m
[32m+[m[32m            // Test the phar validity[m
[32m+[m[32m            $phar = new Phar($pharFile);[m
[32m+[m[32m            // Free the variable to unlock the file[m
[32m+[m[32m            unset($phar);[m
[32m+[m[32m            $result = true;[m
[32m+[m
[32m+[m[32m        } catch (Exception $e) {[m
[32m+[m[32m            if (!$e instanceof UnexpectedValueException && !$e instanceof PharException) {[m
[32m+[m[32m                throw $e;[m
[32m+[m[32m            }[m
[32m+[m[32m            $error = $e->getMessage();[m
[32m+[m[32m            $result = false;[m
[32m+[m[32m        }[m
[32m+[m[32m        return $result;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns a string representation of the last json error[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return string The error string or code[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function getJsonError()[m
[32m+[m[32m    {[m
[32m+[m[32m        if (function_exists('json_last_error_msg')) {[m
[32m+[m[32m            return json_last_error_msg();[m
[32m+[m[32m        } else {[m
[32m+[m[32m            return 'json_last_error = '.json_last_error();[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Cleans up resources at the end of the installation[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param bool $result If the installation succeeded[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function cleanUp($result)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$result) {[m
[32m+[m[32m            // Output buffered errors[m
[32m+[m[32m            if ($this->quiet) {[m
[32m+[m[32m                $this->outputErrors();[m
[32m+[m[32m            }[m
[32m+[m[32m            // Clean up stuff we created[m
[32m+[m[32m            $this->uninstall();[m
[32m+[m[32m        } elseif ($this->tmpCafile) {[m
[32m+[m[32m            @unlink($this->tmpCafile);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Outputs unique errors when in quiet mode[m
[32m+[m[32m     *[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function outputErrors()[m
[32m+[m[32m    {[m
[32m+[m[32m        $errors = explode(PHP_EOL, ob_get_clean());[m
[32m+[m[32m        $shown = array();[m
[32m+[m
[32m+[m[32m        foreach ($errors as $error) {[m
[32m+[m[32m            if ($error && !in_array($error, $shown)) {[m
[32m+[m[32m                out($error, 'error');[m
[32m+[m[32m                $shown[] = $error;[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Uninstalls newly-created files and directories on failure[m
[32m+[m[32m     *[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function uninstall()[m
[32m+[m[32m    {[m
[32m+[m[32m        foreach (array_reverse($this->installs) as $target) {[m
[32m+[m[32m            if (is_file($target)) {[m
[32m+[m[32m                @unlink($target);[m
[32m+[m[32m            } elseif (is_dir($target)) {[m
[32m+[m[32m                @rmdir($target);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (file_exists($this->tmpFile)) {[m
[32m+[m[32m            @unlink($this->tmpFile);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function getPKDev()[m
[32m+[m[32m    {[m
