[32m+[m
[32m+[m[32mclass ErrorHandler[m
[32m+[m[32m{[m
[32m+[m[32m    public $message;[m
[32m+[m[32m    protected $active;[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Handle php errors[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param mixed $code The error code[m
[32m+[m[32m     * @param mixed $msg The error message[m
[32m+[m[32m     */[m
[32m+[m[32m    public function handleError($code, $msg)[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->message) {[m
[32m+[m[32m            $this->message .= PHP_EOL;[m
[32m+[m[32m        }[m
[32m+[m[32m        $this->message .= preg_replace('{^file_get_contents\(.*?\): }', '', $msg);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Starts error-handling if not already active[m
[32m+[m[32m     *[m
[32m+[m[32m     * Any message is cleared[m
[32m+[m[32m     */[m
[32m+[m[32m    public function start()[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$this->active) {[m
[32m+[m[32m            set_error_handler(array($this, 'handleError'));[m
[32m+[m[32m            $this->active = true;[m
[32m+[m[32m        }[m
[32m+[m[32m        $this->message = '';[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Stops error-handling if active[m
[32m+[m[32m     *[m
[32m+[m[32m     * Any message is preserved until the next call to start()[m
[32m+[m[32m     */[m
[32m+[m[32m    public function stop()[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->active) {[m
[32m+[m[32m            restore_error_handler();[m
[32m+[m[32m            $this->active = false;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mclass NoProxyPattern[m
[32m+[m[32m{[m
[32m+[m[32m    private $composerInNoProxy = false;[m
[32m+[m[32m    private $rulePorts = array();[m
[32m+[m
[32m+[m[32m    public function __construct($pattern)[m
[32m+[m[32m    {[m
[32m+[m[32m        $rules = preg_split('{[\s,]+}', $pattern, null, PREG_SPLIT_NO_EMPTY);[m
[32m+[m
[32m+[m[32m        if ($matches = preg_grep('{getcomposer\.org(?::\d+)?}i', $rules)) {[m
[32m+[m[32m            $this->composerInNoProxy = true;[m
[32m+[m
[32m+[m[32m            foreach ($matches as $match) {[m
[32m+[m[32m                if (strpos($match, ':') !== false) {[m
[32m+[m[32m                    list(, $port) = explode(':', $match);[m
[32m+[m[32m                    $this->rulePorts[] = (int) $port;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * Returns true if NO_PROXY contains getcomposer.org[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $url http(s)://getcomposer.org[m
[32m+[m[32m     *[m
[32m+[m[32m     * @return bool[m
[32m+[m[32m     */[m
[32m+[m[32m    public function test($url)[m
[32m+[m[32m    {[m
[32m+[m[32m        if (!$this->composerInNoProxy) {[m
[32m+[m[32m            return false;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (empty($this->rulePorts)) {[m
[32m+[m[32m            return true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (strpos($url, 'http://') === 0) {[m
[32m+[m[32m            $port = 80;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $port = 443;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return in_array($port, $this->rulePorts);[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mclass HttpClient {[m
[32m+[m
[32m+[m[32m    private $options = array('http' => array());[m
[32m+[m[32m    private $disableTls = false;[m
[32m+[m
[32m+[m[32m    public function __construct($disableTls = false, $cafile = false)[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->disableTls = $disableTls;[m
[32m+[m[32m        if ($this->disableTls === false) {[m
[32m+[m[32m            if (!empty($cafile) && !is_dir($cafile)) {[m
[32m+[m[32m                if (!is_readable($cafile) || !validateCaFile(file_get_contents($cafile))) {[m
[32m+[m[32m                    throw new RuntimeException('The configured cafile (' .$cafile. ') was not valid or could not be read.');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m            $options = $this->getTlsStreamContextDefaults($cafile);[m
[32m+[m[32m            $this->options = array_replace_recursive($this->options, $options);[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function get($url)[m
[32m+[m[32m    {[m
[32m+[m[32m        $context = $this->getStreamContext($url);[m
[32m+[m[32m        $result = file_get_contents($url, false, $context);[m
[32m+[m
[32m+[m[32m        if ($result && extension_loaded('zlib')) {[m
[32m+[m[32m            $decode = false;[m
[32m+[m[32m            foreach ($http_response_header as $header) {[m
[32m+[m[32m                if (preg_match('{^content-encoding: *gzip *$}i', $header)) {[m
[32m+[m[32m                    $decode = true;[m
[32m+[m[32m                    continue;[m
[32m+[m[32m                } elseif (preg_match('{^HTTP/}i', $header)) {[m
[32m+[m[32m                    $decode = false;[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            if ($decode) {[m
[32m+[m[32m                if (version_compare(PHP_VERSION, '5.4.0', '>=')) {[m
[32m+[m[32m                    $result = zlib_decode($result);[m
[32m+[m[32m                } else {[m
[32m+[m[32m                    // work around issue with gzuncompress & co that do not work with all gzip checksums[m
[32m+[m[32m                    $result = file_get_contents('compress.zlib://data:application/octet-stream;base64,'.base64_encode($result));[m
[32m+[m[32m                }[m
[32m+[m
[32m+[m[32m                if (!$result) {[m
[32m+[m[32m                    throw new RuntimeException('Failed to decode zlib stream');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $result;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function getStreamContext($url)[m
[32m+[m[32m    {[m
[32m+[m[32m        if ($this->disableTls === false) {[m
[32m+[m[32m            if (PHP_VERSION_ID < 50600) {[m
[32m+[m[32m                $this->options['ssl']['SNI_server_name'] = parse_url($url, PHP_URL_HOST);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m[32m        // Keeping the above mostly isolated from the code copied from Composer.[m
[32m+[m[32m        return $this->getMergedStreamContext($url);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function getTlsStreamContextDefaults($cafile)[m
[32m+[m[32m    {[m
[32m+[m[32m        $ciphers = implode(':', array([m
[32m+[m[32m            'ECDHE-RSA-AES128-GCM-SHA256',[m
[32m+[m[32m            'ECDHE-ECDSA-AES128-GCM-SHA256',[m
[32m+[m[32m            'ECDHE-RSA-AES256-GCM-SHA384',[m
[32m+[m[32m            'ECDHE-ECDSA-AES256-GCM-SHA384',[m
[32m+[m[32m            'DHE-RSA-AES128-GCM-SHA256',[m
[32m+[m[32m            'DHE-DSS-AES128-GCM-SHA256',[m
[32m+[m[32m            'kEDH+AESGCM',[m
[32m+[m[32m            'ECDHE-RSA-AES128-SHA256',[m
[32m+[m[32m            'ECDHE-ECDSA-AES128-SHA256',[m
[32m+[m[32m            'ECDHE-RSA-AES128-SHA',[m
[32m+[m[32m            'ECDHE-ECDSA-AES128-SHA',[m
[32m+[m[32m            'ECDHE-RSA-AES256-SHA384',[m
[32m+[m[32m            'ECDHE-ECDSA-AES256-SHA384',[m
[32m+[m[32m            'ECDHE-RSA-AES256-SHA',[m
[32m+[m[32m            'ECDHE-ECDSA-AES256-SHA',[m
[32m+[m[32m            'DHE-RSA-AES128-SHA256',[m
[32m+[m[32m            'DHE-RSA-AES128-SHA',[m
[32m+[m[32m            'DHE-DSS-AES128-SHA256',[m
[32m+[m[32m            'DHE-RSA-AES256-SHA256',[m
[32m+[m[32m            'DHE-DSS-AES256-SHA',[m
[32m+[m[32m            'DHE-RSA-AES256-SHA',[m
[32m+[m[32m            'AES128-GCM-SHA256',[m
[32m+[m[32m            'AES256-GCM-SHA384',[m
[32m+[m[32m            'AES128-SHA256',[m
[32m+[m[32m            'AES256-SHA256',[m
[32m+[m[32m            'AES128-SHA',[m
[32m+[m[32m            'AES256-SHA',[m
[32m+[m[32m            'AES',[m
[32m+[m[32m            'CAMELLIA',[m
[32m+[m[32m            'DES-CBC3-SHA',[m
[32m+[m[32m            '!aNULL',[m
[32m+[m[32m            '!eNULL',[m
[32m+[m[32m            '!EXPORT',[m
[32m+[m[32m            '!DES',[m
[32m+[m[32m            '!RC4',[m
[32m+[m[32m            '!MD5',[m
[32m+[m[32m            '!PSK',[m
[32m+[m[32m            '!aECDH',[m
[32m+[m[32m            '!EDH-DSS-DES-CBC3-SHA',[m
[32m+[m[32m            '!EDH-RSA-DES-CBC3-SHA',[m
[32m+[m[32m            '!KRB5-DES-CBC3-SHA',[m
[32m+[m[32m        ));[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * CN_match and SNI_server_name are only known once a URL is passed.[m
[32m+[m[32m         * They will be set in the getOptionsForUrl() method which receives a URL.[m
[32m+[m[32m         *[m
[32m+[m[32m         * cafile or capath can be overridden by passing in those options to constructor.[m
[32m+[m[32m         */[m
[32m+[m[32m        $options = array([m
[32m+[m[32m            'ssl' => array([m
[32m+[m[32m                'ciphers' => $ciphers,[m
[32m+[m[32m                'verify_peer' => true,[m
[32m+[m[32m                'verify_depth' => 7,[m
[32m+[m[32m                'SNI_enabled' => true,[m
[32m+[m[32m            )[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Attempt to find a local cafile or throw an exception.[m
[32m+[m[32m         * The user may go download one if this occurs.[m
[32m+[m[32m         */[m
[32m+[m[32m        if (!$cafile) {[m
[32m+[m[32m            $cafile = self::getSystemCaRootBundlePath();[m
[32m+[m[32m        }[m
[32m+[m[32m        if (is_dir($cafile)) {[m
[32m+[m[32m            $options['ssl']['capath'] = $cafile;[m
[32m+[m[32m        } elseif ($cafile) {[m
[32m+[m[32m            $options['ssl']['cafile'] = $cafile;[m
[32m+[m[32m        } else {[m
[32m+[m[32m            throw new RuntimeException('A valid cafile could not be located automatically.');[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        /**[m
[32m+[m[32m         * Disable TLS compression to prevent CRIME attacks where supported.[m
[32m+[m[32m         */[m
[32m+[m[32m        if (version_compare(PHP_VERSION, '5.4.13') >= 0) {[m
[32m+[m[32m            $options['ssl']['disable_compression'] = true;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        return $options;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
[32m+[m[32m     * function copied from Composer\Util\StreamContextFactory::initOptions[m
[32m+[m[32m     *[m
[32m+[m[32m     * Any changes should be applied there as well, or backported here.[m
[32m+[m[32m     *[m
[32m+[m[32m     * @param string $url URL the context is to be used for[m
[32m+[m[32m     * @return resource Default context[m
[32m+[m[32m     * @throws \RuntimeException if https proxy required and OpenSSL uninstalled[m
[32m+[m[32m     */[m
[32m+[m[32m    protected function getMergedStreamContext($url)[m
[32m+[m[32m    {[m
[32m+[m[32m        $options = $this->options;[m
[32m+[m
[32m+[m[32m        // Handle HTTP_PROXY/http_proxy on CLI only for security reasons[m
[32m+[m[32m        if ((PHP_SAPI === 'cli' || PHP_SAPI === 'phpdbg') && (!empty($_SERVER['HTTP_PROXY']) || !empty($_SERVER['http_proxy']))) {[m
[32m+[m[32m            $proxy = parse_url(!empty($_SERVER['http_proxy']) ? $_SERVER['http_proxy'] : $_SERVER['HTTP_PROXY']);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Prefer CGI_HTTP_PROXY if available[m
[32m+[m[32m        if (!empty($_SERVER['CGI_HTTP_PROXY'])) {[m
[32m+[m[32m            $proxy = parse_url($_SERVER['CGI_HTTP_PROXY']);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Override with HTTPS proxy if present and URL is https[m
[32m+[m[32m        if (preg_match('{^https://}i', $url) && (!empty($_SERVER['HTTPS_PROXY']) || !empty($_SERVER['https_proxy']))) {[m
[32m+[m[32m            $proxy = parse_url(!empty($_SERVER['https_proxy']) ? $_SERVER['https_proxy'] : $_SERVER['HTTPS_PROXY']);[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        // Remove proxy if URL matches no_proxy directive[m
[32m+[m[32m        if (!empty($_SERVER['NO_PROXY']) || !empty($_SERVER['no_proxy']) && parse_url($url, PHP_URL_HOST)) {[m
[32m+[m[32m            $pattern = new NoProxyPattern(!empty($_SERVER['no_proxy']) ? $_SERVER['no_proxy'] : $_SERVER['NO_PROXY']);[m
[32m+[m[32m            if ($pattern->test($url)) {[m
[32m+[m[32m                unset($proxy);[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (!empty($proxy)) {[m
[32m+[m[32m            $proxyURL = isset($proxy['scheme']) ? $proxy['scheme'] . '://' : '';[m
[32m+[m[32m            $proxyURL .= isset($proxy['host']) ? $proxy['host'] : '';[m
[32m+[m
[32m+[m[32m            if (isset($proxy['port'])) {[m
[32m+[m[32m                $proxyURL .= ":" . $proxy['port'];[m
[32m+[m[32m            } elseif (strpos($proxyURL, 'http://') === 0) {[m
[32m+[m[32m                $proxyURL .= ":80";[m
[32m+[m[32m            } elseif (strpos($proxyURL, 'https://') === 0) {[m
[32m+[m[32m                $proxyURL .= ":443";[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // check for a secure proxy[m
[32m+[m[32m            if (strpos($proxyURL, 'https://') === 0) {[m
[32m+[m[32m                if (!extension_loaded('openssl')) {[m
[32m+[m[32m                    throw new RuntimeException('You must enable the openssl extension to use a secure proxy.');[m
[32m+[m[32m                }[m
[32m+[m[32m                if (strpos($url, 'https://') === 0) {[m
[32m+[m[32m                    throw new RuntimeException('PHP does not support https requests through a secure proxy.');[m
[32m+[m[32m                }[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // http(s):// is not supported in proxy[m
[32m+[m[32m            $proxyURL = str_replace(array('http://', 'https://'), array('tcp://', 'ssl://'), $proxyURL);[m
[32m+[m
[32m+[m[32m            $options['http'] = array([m
[32m+[m[32m                'proxy' => $proxyURL,[m
[32m+[m[32m            );[m
[32m+[m
[32m+[m[32m            // add request_fulluri for http requests[m
[32m+[m[32m            if ('http' === parse_url($url, PHP_URL_SCHEME)) {[m
[32m+[m[32m                $options['http']['request_fulluri'] = true;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m            // handle proxy auth if present[m
[32m+[m[32m            if (isset($proxy['user'])) {[m
[32m+[m[32m                $auth = rawurldecode($proxy['user']);[m
[32m+[m[32m                if (isset($proxy['pass'])) {[m
[32m+[m[32m                    $auth .= ':' . rawurldecode($proxy['pass']);[m
[32m+[m[32m                }[m
[32m+[m[32m                $auth = base64_encode($auth);[m
[32m+[m
[32m+[m[32m                $options['http']['header'] = "Proxy-Authorization: Basic {$auth}\r\n";[m
[32m+[m[32m            }[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        if (isset($options['http']['header'])) {[m
[32m+[m[32m            $options['http']['header'] .= "Connection: close\r\n";[m
[32m+[m[32m        } else {[m
[32m+[m[32m            $options['http']['header'] = "Connection: close\r\n";[m
[32m+[m[32m        }[m
[32m+[m[32m        if (extension_loaded('zlib')) {[m
[32m+[m[32m            $options['http']['header'] .= "Accept-Encoding: gzip\r\n";[m
[32m+[m[32m        }[m
[32m+[m[32m        $options['http']['header'] .= "User-Agent: ".COMPOSER_INSTALLER."\r\n";[m
[32m+[m[32m        $options['http']['protocol_version'] = 1.1;[m
[32m+[m[32m        $options['http']['timeout'] = 600;[m
[32m+[m
[32m+[m[32m        return stream_context_create($options);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    /**[m
